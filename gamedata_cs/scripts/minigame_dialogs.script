-- Is minigame valiable?
function crowkiller_is_valiable()
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
    return (m:get_minigame_state(crowkiller)=="valiable") or (m:get_minigame_state(crowkiller)=="disabled")
end
-- Is minigame finished?
function crowkiller_is_finished()
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
    return m:get_minigame_state(crowkiller)=="finished"
end
-- Is minigame winned?
function crowkiller_is_winned()
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
    return m:get_minigame_param(crowkiller, "win")
end
-- Opposite to previous function
function crowkiller_is_not_winned()
    return not(crowkiller_is_winned())
end
-- Is minigame breaked?
function crowkiller_is_breaked()
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
    return m:get_minigame_state(crowkiller)=="breaked"
end
-- Is minigame running?
function crowkiller_is_running()
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
    return (m:get_minigame_state(crowkiller)=="running") or (m:get_minigame_state(crowkiller)=="started")
end
-- Set crowkiller game type
function set_crowkiller_game_type(fs, ss, dn, pi)
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
	if(dn=="gar_bandit_bookmaker_training") then
		m:set_minigame_param(crowkiller, "game_type", "training")
	elseif(dn=="gar_bandit_bookmaker_money") then
		m:set_minigame_param(crowkiller, "game_type", "money")
	elseif(dn=="gar_bandit_bookmaker_record") then
		m:set_minigame_param(crowkiller, "game_type", "highscore")
	end
end
-- Get phrases with crows to kill count
function get_crows_to_kill_count(fs, dn, pi)
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
	local string = ""
	if(pi=="1111") or (pi=="1112") or (pi=="1113") or (pi=="1114") then
-- if game type is training or money
		local i = tonumber(string.sub(pi, -1))
		string = game.translate_string("crowkiller_dialog_crows_per_minute")..", "..tostring(m:get_minigame_param(crowkiller, "crows_to_kill")[i])
	elseif(pi=="1") then
-- if game type is highscore
		string = game.translate_string("crowkiller_dialog_record_crows").." "..tostring(m:get_minigame_param(crowkiller, "highscore"))
	end
    return string
end
-- Select crows to kill count
function set_crowkiller_selected_count(fs, ss, dn, pi)
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
	if(m:get_minigame_param(crowkiller, "game_type")=="highscore") then
	    m:set_minigame_param(crowkiller, "selected", m:get_minigame_param(crowkiller, "highscore"))
	else
	    local i = tonumber(string.sub(pi, -1))
	    m:set_minigame_param(crowkiller, "selected", m:get_minigame_param(crowkiller, "crows_to_kill")[i])
	end
end
--!!! Get phrase with reward money
