-- Is minigame valiable?
function is_shooting_valiable()
	local manager = minigames_manager.get_minigames_manager()
	local shooting = manager:get_current_shooting()
    return (manager:get_minigame_state(shooting)=="valiable") or (manager:get_minigame_state(shooting)=="disabled")
end
-- Is minigame finished?
function is_shooting_finished()
	local manager = minigames_manager.get_minigames_manager()
	local shooting = manager:get_current_shooting()
    return manager:get_minigame_state(shooting)=="finished"
end
-- Is minigame running?
function is_shooting_running()
	local manager = minigames_manager.get_minigames_manager()
	local shooting = manager:get_current_shooting()
    return (manager:get_minigame_state(shooting)=="started") or (manager:get_minigame_state(shooting)=="running")
end
