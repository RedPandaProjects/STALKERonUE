end
-- Set minigame valiable (start minigame)
function start_crowkiller()
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
    m:set_minigame_state(crowkiller, "enabled")
end
-- Set minigame valiable after finishing
function set_crowkiller_valiable()
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
    m:set_minigame_state(crowkiller, "disabled")
end
-- Transfer stake from actor
function transfer_stake(fs, ss, dn, pi)
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
	local num = 0
	if(m:get_minigame_param(crowkiller, "game_type")=="money") then
		num = m:get_minigame_param(crowkiller, "selected")*m:get_minigame_param(crowkiller, "money_multiplier")
	    dialogs.relocate_money(ss, num, "out")
	elseif(m:get_minigame_param(crowkiller, "game_type")=="highscore") then
		num = m:get_minigame_param(crowkiller, "selected")*m:get_minigame_param(crowkiller, "champion_multiplier")
	    dialogs.relocate_money(ss, num, "out")
	end
end
-- Transfer winned money to actor
function transfer_winned_money(fs, ss, dn, pi)
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
	local num = 0
	if(m:get_minigame_param(crowkiller, "game_type")=="money") then
		num = m:get_minigame_param(crowkiller, "selected")*m:get_minigame_param(crowkiller, "money_multiplier")
	    dialogs.relocate_money(fs, num*2, "in")
	elseif(m:get_minigame_param(crowkiller, "game_type")=="highscore") then
		num = m:get_minigame_param(crowkiller, "selected")*m:get_minigame_param(crowkiller, "champion_multiplier")
	    dialogs.relocate_money(fs, num*2, "in")
	end
end

function crowkiller_game_type_training(fs, ss, dn, pi)
	local m = minigames_manager.get_minigames_manager()
	local crowkiller = m:get_current_crowkiller()
    return m:get_minigame_param(crowkiller, "game_type")=="training"
end

function crowkiller_game_type_not_training(fs, ss, dn, pi)
    return not(crowkiller_game_type_training(fs, ss, dn, pi))
end
